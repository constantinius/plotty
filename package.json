{
  "name": "plotty",
  "version": "0.2.0",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "pegjs": "^0.10.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "main": "src/plotty.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/santilland/plotty.git"
  },
  "scripts": {
    "build-min": "browserify -t babelify -g uglifyify src/plotty.js -o dist/plotty.min.js --standalone window",
    "build": "browserify -t babelify src/plotty.js -o dist/plotty.js --standalone window",
    "make-parser": "pegjs -o src/arithmetics-parser.js src/arithmetics.pegjs",
    "watch": "watchify -d -t babelify src/plotty.js -o dist/plotty.js -v --standalone window",
    "start": "budo src/plotty.js:plotty.js -l -d test/ -d dist/ -o --static-options [ --cache-control false ] -- -t babelify --standalone window",
    "jsdoc": "rm -rf docs/*; jsdoc -r src README.md -d docs -t node_modules/ink-docstrap/template -c jsdoc.json",
    "prepublish": "npm run build && npm run build-min"
  }
}
